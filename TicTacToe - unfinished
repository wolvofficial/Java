package tictactoe;

public class tictactoegame {

	public static void main(String[] args) {
		
		char[][] gameBoard = {{' ', '|', ' ', '|', ' '},
				{'_', '+', '_', '+', '_'},
				{' ', '|', ' ', '|', ' '},
				{'_', '+', '_', '+', '_'},
				{' ', '|', ' ', '|', ' '}};
	
		printGameBoard(gameBoard);
	}

	public static void printGameBoard(char[][] gameBoard) {
		for(char[] row : gameBoard) {
			for(char c : row) {
				System.out.println(c);				
			}
			System.out.println();
		}
	}
}
	
